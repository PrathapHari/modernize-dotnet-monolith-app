{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Debug",
      "Microsoft.AspNetCore.Diagnostics": "Trace",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information",
      "Microsoft.EntityFrameworkCore.Infrastructure": "Debug",
      "Microsoft.EntityFrameworkCore.Query": "Debug",
      "eShopLite.StoreFx": "Trace",
      "eShopLite.StoreFx.Controllers": "Trace",
      "eShopLite.StoreFx.Services": "Trace",
      "eShopLite.StoreFx.Data": "Debug"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] ",
      "LogLevel": {
        "Default": "Trace"
      }
    },
    "Debug": {
      "IncludeScopes": true,
      "LogLevel": {
        "Default": "Trace"
      }
    }
  },
  "DetailedErrors": true,
  "EnableSensitiveDataLogging": true
}
